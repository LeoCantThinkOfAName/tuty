    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "gh-pages": "^5.0.0",
    "happy-dom": "^9.10.7",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "prop-types": "^15.8.1",
